If i = 1 Then
    ' Copy and paste values from A4:J5 to A5:J5
    targetSheet.Range("A5:J5").Value = targetSheet.Range("A4:J5").Value
    
    ' Paste value of TextBox9 into cell H5
    targetSheet.Range("H5").Value = Me.Controls("TextBox9").Text
    
    lastRow = lastRow + 2 ' Move to the next empty row 
