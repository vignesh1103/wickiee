' If CheckBoxes 1 and 3  are selected together,
                    If checkboxes13Selected And i = 1 Then
                        targetSheet.Range("A" & lastRow).Resize(1, 10).value = ThisWorkbook.Sheets("Sheet2").Range("A20:J20").value
                        lastRow = lastRow + 1 ' Move to the next empty row
                        targetSheet.Range("A" & lastRow).Resize(3, 10).value = ThisWorkbook.Sheets("Sheet2").Range("A26:J28").value
                        targetSheet.Range("H" & lastRow + 0 & ":H" & lastRow + 1).value = TextBox13.value
                        targetSheet.Range("H" & lastRow + 2).value = TextBox14.value  ' Replace column H values with TextBox14 value
                    ' Append TextBox13 and 14 value to cells in column J of rows 24 and 25
                        targetSheet.Cells(lastRow + 0, "J").value = targetSheet.Cells(lastRow + 1, "J").value & ", " & TextBox13.value
                        targetSheet.Cells(lastRow + 1, "J").value = targetSheet.Cells(lastRow + 2, "J").value & ", " & TextBox13.value
                        targetSheet.Cells(lastRow + 2, "J").value = targetSheet.Cells(lastRow + 2, "J").value & ", " & TextBox14.value
                        lastRow = lastRow + 3 ' Move to the next empty row
                    End If
If checkboxes13Selected And i = 1 Then
    ' Paste data from Sheet2 into target sheet for rows 20 and 26-28
    targetSheet.Range("A" & lastRow).Resize(1, 10).Value = ThisWorkbook.Sheets("Sheet2").Range("A20:J20").Value
    lastRow = lastRow + 1 ' Move to the next empty row
    targetSheet.Range("A" & lastRow + 1).Resize(3, 10).Value = ThisWorkbook.Sheets("Sheet2").Range("A26:J28").Value
    
    ' Replace values in column H
    targetSheet.Range("H" & lastRow).Value = TextBox13.Value
    targetSheet.Range("H" & lastRow + 1 & ":H" & lastRow + 1).Value = TextBox13.Value
    targetSheet.Range("H" & lastRow + 2).Value = TextBox14.Value
    
    ' Append values in column J
    targetSheet.Range("J" & lastRow).Value = targetSheet.Range("J" & lastRow).Value & ", " & TextBox13.Value
    targetSheet.Range("J" & lastRow + 1 & ":J" & lastRow + 1).Value = targetSheet.Range("J" & lastRow + 1).Value & ", " & TextBox13.Value
    targetSheet.Range("J" & lastRow + 2).Value = targetSheet.Range("J" & lastRow + 2).Value & ", " & TextBox14.Value
    
    lastRow = lastRow + 3 ' Move to the next empty row
End If
