    qgc = ThisWorkbook.Sheets("Sheet1").Range("C14").Value
    
    Select Case qgc
        Case "QGC0"
            If CheckBox2.Value = True Then
                CopyCheckBoxData CheckBox2, "A6:J7", 2
            End If
        Case "QGC2"
            If CheckBox2.Value = True Then
                CopyCheckBoxData CheckBox2, "A23:J25", 3
            End If
        Case "QGC4"
            If CheckBox2.Value = True Then
                CopyCheckBoxData CheckBox2, "A34:J36", 3
            End If
    End Select
End Sub

