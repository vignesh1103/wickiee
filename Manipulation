End Sub

Private Sub CheckBox6_Click()
    ' CheckBox6 Click Event
    ' Handle the logic for CheckBox6
    Dim qgc As String
    qgc = ThisWorkbook.Sheets("Sheet1").Range("C14").Value
    
    Select Case qgc
        Case "QGC0"
            If CheckBox6.Value = True Then
                CheckBox6.CopyData "A13:J13", 1
                CheckBox6.CopyData "A15:J15", 1
            End If
        Case "QGC2"
            CheckBox6.Enabled = False
        Case "QGC4"
            If CheckBox6.Value = True Then
                CheckBox6.CopyData "A43:J43", 1
                CheckBox6.CopyData "A45:J45", 1
            End If
    End Select
End Sub

Private Sub CommandButton7_Click()
    ' CommandButton7 Click Event
    ' Paste the selected checkbox data to Sheet3
    Dim targetSheet As Worksheet
    Set targetSheet = ThisWorkbook.Sheets("Sheet3")
    
    Dim lastRow As Long
    lastRow = targetSheet.Cells(targetSheet.Rows.Count, "A").End(xlUp).Row + 1 ' Find the last empty row in Sheet3
    
    ' Paste copied data from CheckBox1
    CheckBox1.PasteData targetSheet, lastRow
    
    ' Paste copied data from CheckBox2
    CheckBox2.PasteData targetSheet, lastRow
    
    ' Paste copied data from CheckBox3
    CheckBox3.PasteData targetSheet, lastRow
    
    ' Paste copied data from CheckBox4
    CheckBox4.PasteData targetSheet, lastRow
    
    ' Paste copied data from CheckBox5
    CheckBox5.PasteData targetSheet, lastRow
    
    ' Paste copied data from CheckBox6
    CheckBox6.PasteData targetSheet, lastRow
End Sub
