Private Sub CheckBox3_Click()
    Dim qgc As String
    qgc = ThisWorkbook.Sheets("Sheet1").Range("C14").Value
    
    Dim targetSheet As Worksheet
    Set targetSheet = ThisWorkbook.Sheets("Sheet3")
    
    Dim lastRow As Long
    lastRow = targetSheet.Cells(targetSheet.Rows.Count, "A").End(xlUp).Row + 1 ' Find the last empty row in Sheet3
    
    ' Check if Checkbox3, Checkbox4, and Checkbox6 are selected together and qgc is "QGC0"
    If CheckBox3.Value = True And CheckBox4.Value = True And CheckBox6.Value = True And qgc = "QGC0" Then
        ' Copy row 13 only once
        targetSheet.Range("A" & lastRow).Resize(1, 10).Value = ThisWorkbook.Sheets("Sheet2").Range("A13:J13").Value
        lastRow = lastRow + 1 ' Move to the next empty row
        
        ' Copy rows 14, 15, and 16
        targetSheet.Range("A" & lastRow).Resize(3, 10).Value = ThisWorkbook.Sheets("Sheet2").Range("A14:J16").Value
        lastRow = lastRow + 3 ' Move to the next empty row
    Else
        ' Perform the original logic when checkboxes 3, 4, or 6 are not selected together
        Select Case qgc
            Case "QGC0"
                If CheckBox3.Value = True Then
                    targetSheet.Range("A" & lastRow).Resize(2, 10).Value = ThisWorkbook.Sheets("Sheet2").Range("A13:J14").Value
                    lastRow = lastRow + 2 ' Move to the next empty row
                End If
                ' Continue with the rest of the original logic for QGC0
                ' Add Case statements for other values of qgc if needed
        End Select
    End If
End Sub
