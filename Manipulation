Private Sub Button7_Click(sender As Object, e As EventArgs) Handles Button7.Click
    ' Define the TextBoxes array
    Dim TextBoxes() As TextBox = {TextBox9, TextBox10, TextBox11, TextBox12, TextBox13, TextBox14, TextBox15, TextBox16, TextBox17, TextBox18, TextBox19, TextBox20, TextBox21, TextBox22}

    ' Initialize checkboxes
    CheckBox1.Tag = "9,10"
    CheckBox2.Tag = "11,12"
    CheckBox3.Tag = "13,14"
    CheckBox4.Tag = "15,16"
    CheckBox5.Tag = "17,18"
    CheckBox6.Tag = "19,20"
    CheckBox7.Tag = "21,22"

    ' Handle the CheckedChanged event for all checkboxes
    For Each checkBox As CheckBox In {CheckBox1, CheckBox2, CheckBox3, CheckBox4, CheckBox5, CheckBox6, CheckBox7}
        AddHandler checkBox.CheckedChanged, AddressOf CheckBox_CheckedChanged
    Next
End Sub

' Handle the CheckedChanged event for all checkboxes
Private Sub CheckBox_CheckedChanged(sender As Object, e As EventArgs)
    Dim checkBox As CheckBox = DirectCast(sender, CheckBox)
    Dim indices As String() = checkBox.Tag.ToString().Split(","c)

    If indices.Length = 2 Then
        Dim startIndex As Integer = Convert.ToInt32(indices(0))
        Dim endIndex As Integer = Convert.ToInt32(indices(1))

        For i As Integer = startIndex To endIndex
            If i >= 0 AndAlso i < TextBoxes.Length Then
                TextBoxes(i).Enabled = checkBox.Checked
            End If
        Next
    End If
End Sub
