private sub checkbox_click()
    dim chkbox as msforms.checkbox ' assuming you're using msforms checkboxes
    
    for each chkbox in me.controls
        if typename(chkbox) = "checkbox" then
            if chkbox.value = true then
                select case chkbox.name
                    case "checkbox1"
                        enabletextboxes me.controls("textbox9"), me.controls("textbox10")
                    case "checkbox2"
                        enabletextboxes me.controls("textbox11"), me.controls("textbox12")
                    case "checkbox3"
                        enabletextboxes me.controls("textbox13"), me.controls("textbox14")
                    case "checkbox4"
                        enabletextboxes me.controls("textbox15"), me.controls("textbox16")
                    case "checkbox5"
                        enabletextboxes me.controls("textbox17"), me.controls("textbox18")
                    case "checkbox6"
                        enabletextboxes me.controls("textbox19"), me.controls("textbox20")
                    case "checkbox7"
                        enabletextboxes me.controls("textbox21"), me.controls("textbox22")
                end select
            else
                select case chkbox.name
                    case "checkbox1"
                        disabletextboxes me.controls("textbox9"), me.controls("textbox10")
                    case "checkbox2"
                        disabletextboxes me.controls("textbox11"), me.controls("textbox12")
                    case "checkbox3"
                        disabletextboxes me.controls("textbox13"), me.controls("textbox14")
                    case "checkbox4"
                        disabletextboxes me.controls("textbox15"), me.controls("textbox16")
                    case "checkbox5"
                        disabletextboxes me.controls("textbox17"), me.controls("textbox18")
                    case "checkbox6"
                        disabletextboxes me.controls("textbox19"), me.controls("textbox20")
                    case "checkbox7"
                        disabletextboxes me.controls("textbox21"), me.controls("textbox22")
                end select
            end if
        end if
    next chkbox
end sub

sub enabletextboxes(paramarray textboxes() as variant)
    dim textbox as variant
    for each textbox in textboxes
        textbox.enabled = true
        ' set any other properties as needed
    next textbox
end sub

sub disabletextboxes(paramarray textboxes() as variant)
    dim textbox as variant
    for each textbox in textboxes
        textbox.enabled = false
        ' set any other properties as needed
    next textbox
end sub
