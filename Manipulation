Private Sub CheckBox_CheckedChanged(sender As Object, e As EventArgs) Handles CheckBox1.CheckedChanged, CheckBox2.CheckedChanged, CheckBox3.CheckedChanged, CheckBox4.CheckedChanged, CheckBox5.CheckedChanged, CheckBox6.CheckedChanged, CheckBox7.CheckedChanged
    Dim checkBox As CheckBox = DirectCast(sender, CheckBox)
    ' Your code to handle the CheckedChanged event goes here
End Sub

Private Sub CheckBox_CheckedChanged(sender As Object, e As EventArgs) Handles CheckBox1.CheckedChanged, CheckBox2.CheckedChanged, CheckBox3.CheckedChanged, CheckBox4.CheckedChanged, CheckBox5.CheckedChanged, CheckBox6.CheckedChanged, CheckBox7.CheckedChanged
    Dim checkBox As CheckBox = DirectCast(sender, CheckBox)

    ' Enable or disable the associated textboxes based on the checkbox checked state
    Select Case checkBox.Name
        Case "CheckBox1"
            TextBox9.Enabled = checkBox.Checked
            TextBox10.Enabled = checkBox.Checked
        Case "CheckBox2"
            TextBox11.Enabled = checkBox.Checked
            TextBox12.Enabled = checkBox.Checked
        Case "CheckBox3"
            TextBox13.Enabled = checkBox.Checked
            TextBox14.Enabled = checkBox.Checked
        Case "CheckBox4"
            TextBox15.Enabled = checkBox.Checked
            TextBox16.Enabled = checkBox.Checked
        Case "CheckBox5"
            TextBox17.Enabled = checkBox.Checked
            TextBox18.Enabled = checkBox.Checked
        Case "CheckBox6"
            TextBox19.Enabled = checkBox.Checked
            TextBox20.Enabled = checkBox.Checked
        Case "CheckBox7"
            TextBox21.Enabled = checkBox.Checked
            TextBox22.Enabled = checkBox.Checked
    End Select
End Sub
