Private Sub CheckBox1_Click()
    ' CheckBox1 Click Event
    ' Handle the logic for CheckBox1
    Dim qgc As String
    qgc = ThisWorkbook.Sheets("Sheet1").Range("C14").Value
    
    Select Case qgc
        Case "QGC0"
            If CheckBox1.Value = True Then
                CheckBox1.CopyData "A4:J5", 2
            End If
        Case "QGC2"
            If CheckBox1.Value = True Then
                CheckBox1.CopyData "A20:J20", 1
                CheckBox1.CopyData "A26:J27", 2
            End If
        Case "QGC4"
            If CheckBox1.Value = True Then
                CheckBox1.CopyData "A32:J33", 2
            End If
    End Select
End Sub

