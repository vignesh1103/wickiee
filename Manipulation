Private Sub CheckBox1_Click()
    Dim targetSheet As Worksheet
    Dim sourceRange As Range
    Dim lastRow As Long
    
    ' Set the target sheet (Sheet3)
    Set targetSheet = ThisWorkbook.Sheets("Sheet3")
    
    ' Determine the row range to copy based on the selected QGC
    Dim qgcRange As Range
    Select Case ThisWorkbook.Sheets("Sheet1").Range("C14").value
        Case "QGC0"
            Set qgcRange = ThisWorkbook.Sheets("Sheet2").Range("A4:J5")
        Case "QGC2"
            Set qgcRange = ThisWorkbook.Sheets("Sheet2").Range("A6:J7")
        Case "QGC4"
            Set qgcRange = ThisWorkbook.Sheets("Sheet2").Range("A8:J9")
        Case Else
            ' Default range if QGC is not recognized
            Set qgcRange = Nothing
    End Select
    
    ' Check if CheckBox1 is checked and a valid QGC range is defined
    If CheckBox1.value = True And Not qgcRange Is Nothing Then
        ' Checkbox is checked, copy data from Sheet2 to Sheet3
        lastRow = targetSheet.Cells(targetSheet.Rows.Count, "A").End(xlUp).Row + 1
        qgcRange.Copy targetSheet.Cells(lastRow, "A")
    Else
        ' Checkbox is unchecked or QGC range is not defined, remove corresponding data from Sheet3 (if needed)
        ' Add logic to remove data from Sheet3 here (if required)
    End If
End Sub
