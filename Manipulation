' For rows in 48th and 49th, copy value from B column to copy_id and paste in E column
If i = 2 Then
    copy_id = destinationRange.Offset(i - 2, 1).Value ' Copy value from B column to copy_id
    wsDestination.Cells(destinationRange.Row + i - 2, 5).ClearContents ' Clear the value in E column for this row
    destinationRange.Offset(i - 1, 4).Value = "#" & copy_id ' Paste value into E column
Else
    destinationRange.Offset(i - 1, 4).Value = "#" & copy_id ' Paste value into E column
End If
