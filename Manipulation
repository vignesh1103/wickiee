Private Sub CheckBox_Click()
    Dim chkBox As CheckBox
    
    For Each chkBox In ActiveSheet.CheckBoxes
        If chkBox.Value = 1 Then
            Select Case chkBox.Name
                Case "CheckBox1"
                    EnableTextBoxes "TextBox9", "TextBox10"
                Case "CheckBox2"
                    EnableTextBoxes "TextBox11", "TextBox12"
                Case "CheckBox3"
                    EnableTextBoxes "TextBox13", "TextBox14"
                Case "CheckBox4"
                    EnableTextBoxes "TextBox15", "TextBox16"
                Case "CheckBox5"
                    EnableTextBoxes "TextBox17", "TextBox18"
                Case "CheckBox6"
                    EnableTextBoxes "TextBox19", "TextBox20"
                Case "CheckBox7"
                    EnableTextBoxes "TextBox21", "TextBox22"
            End Select
        Else
            Select Case chkBox.Name
                Case "CheckBox1"
                    DisableTextBoxes "TextBox9", "TextBox10"
                Case "CheckBox2"
                    DisableTextBoxes "TextBox11", "TextBox12"
                Case "CheckBox3"
                    DisableTextBoxes "TextBox13", "TextBox14"
                Case "CheckBox4"
                    DisableTextBoxes "TextBox15", "TextBox16"
                Case "CheckBox5"
                    DisableTextBoxes "TextBox17", "TextBox18"
                Case "CheckBox6"
                    DisableTextBoxes "TextBox19", "TextBox20"
                Case "CheckBox7"
                    DisableTextBoxes "TextBox21", "TextBox22"
            End Select
        End If
    Next chkBox
End Sub

Sub EnableTextBoxes(ParamArray textBoxNames() As Variant)
    Dim textBoxName As Variant
    For Each textBoxName In textBoxNames
        ActiveSheet.Shapes(textBoxName).DrawingObject.Enabled = True
        ActiveSheet.Shapes(textBoxName).Fill.ForeColor.RGB = RGB(255, 255, 255) ' Enable textboxes
    Next textBoxName
End Sub

Sub DisableTextBoxes(ParamArray textBoxNames() As Variant)
    Dim textBoxName As Variant
    For Each textBoxName In textBoxNames
        ActiveSheet.Shapes(textBoxName).DrawingObject.Enabled = False
        ActiveSheet.Shapes(textBoxName).Fill.ForeColor.RGB = RGB(200, 200, 200) ' Disable textboxes
    Next textBoxName
End Sub
